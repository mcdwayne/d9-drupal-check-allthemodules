{#
/**
 * @file membership_entity.html.twig
 * Default theme implementation to present Membership data.
 *
 * This template is used when viewing Membership pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_membership_entity()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('membership_entity') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
