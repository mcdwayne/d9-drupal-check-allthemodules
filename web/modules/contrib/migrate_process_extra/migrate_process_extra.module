<?php

/**
 * @file
 * Contains migrate_process_extra.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function migrate_process_extra_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the migrate_process_extra module.
    case 'help.page.migrate_process_extra':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Provides a collection of Migrate process plugins that are not part of the core or Migrate Plus process plugins.') . '</p>';
      return $output;

    default:
  }
}
