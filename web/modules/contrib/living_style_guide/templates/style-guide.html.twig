<h2>Markup at text-level</h2>
<ul>
    <li><abbr title="Hyper Text Markup Language">HTML</abbr> (<code>abbr</code>)</li>
    <li><acronym title="Light amplification by the stimulated emission of radiation">LASER</acronym>
        (<code>acronym</code>)</li>
    <li><b>bold</b> (<code>b</code>)</li>
    <li><big>big</big> (<code>big</code>)</li>
    <li><font size=6>large size</font> (<code>font size=6</code>)</li>
    <li><font face=Courier>Courier font</font> (<code>font face=Courier</code>)</li>
    <li><font color=red>red</font> (<code>font color=red</code>)</li>
    <li><cite>Origin of Species</cite> (<code>cite</code>)</li>
    <li><code>a[i]=b[i] + c[i];</code> (<code>code</code>)</li>
    <li><del>deleted</del> (<code>del</code>)</li>
    <li><dfn>octet</dfn> (<code>dfn</code>)</li>
    <li><em>emphasis</em> (<code>em</code>)</li>
    <li><i>italics</i> (<code>i</code>)</li>
    <li><ins>inserted</ins> (<code>ins</code>)</li>
    <li><kbd>input</kbd> (<code>kbd</code>)</li>
    <li><q>Quote!</q> (<code>q</code>)</li>
    <li><q>Quote <q>quote inside quote</q></q> (<code>q</code> inside <code>q</code>)</li>
    <li><samp>Message</samp> (<code>samp</code>)</li>
    <li><small>Small</small> (<code>small</code>)</li>
    <li><strike>Striked</strike> (<code>strike</code>, <code>s</code> is a non-standard synonym)</li>
    <li><strong>Strong</strong> (<code>strong</code>)</li>
    <li>X<sub>1</sub> (<code>sub</code>)</li>
    <li><tt>Monospace font</tt> (<code>tt</code>)</li>
    <li><u>underlined</u> (<code>u</code>)</li>
    <li><var>filename</var> (<code>var</code>).</li>
</ul>
