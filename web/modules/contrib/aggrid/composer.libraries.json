{
  "name": "drupal/aggrid",
  "type": "drupal-module",
  "description": "Adds ag-Grid to Drupal 8 Form API",
  "keywords": ["Drupal"],
  "license": "GPL-2.0+",
  "homepage": "https://www.drupal.org/project/aggrid",
  "authors": [
    {
      "name": "Michael Feranda (mferanda)",
      "homepage": "https://www.drupal.org/u/mferanda",
      "role": "Maintainer"
    }
  ],
  "minimum-stability": "dev",
  "support": {
    "issues": "https://www.drupal.org/project/issues/aggrid",
    "source": "http://cgit.drupalcode.org/aggrid",
    "docs": "https://www.drupal.org/docs/8/modules/aggrid"
  },
  "autoload": {
    "psr-4": {
      "Drupal\\aggrid\\": "src/"
    }
  },
  "extra": {
    "drush": {
      "services": {
        "drush.services.yml": "^9"
      }
    }
  },
  "repositories": {
    "ag-grid-community": {
      "type": "package",
      "package": {
        "name": "ag-grid-community/ag-grid-community",
        "version": "20.0.0",
        "type": "drupal-library",
        "extra": {
          "installer-name": "ag-grid-community"
        },
        "dist": {
          "url": "https://registry.npmjs.org/ag-grid-community/-/ag-grid-community-20.0.0.tgz",
          "type": "tar"
        },
        "require": {
          "composer/installers": "~1.0"
        }
      }
    },
    "ag-grid-enterprise": {
      "type": "package",
      "package": {
        "name": "ag-grid-enterprise/ag-grid-enterprise",
        "version": "20.0.0",
        "type": "drupal-library",
        "extra": {
          "installer-name": "ag-grid-enterprise"
        },
        "dist": {
          "url": "https://registry.npmjs.org/ag-grid-enterprise/-/ag-grid-enterprise-20.0.0.tgz",
          "type": "tar"
        },
        "require": {
          "composer/installers": "~1.0"
        }
      }
    }
  },
  "require": {
    "ag-grid-community/ag-grid-community": "20.0.0",
    "ag-grid-enterprise/ag-grid-enterprise": "20.0.0"
  }
}
