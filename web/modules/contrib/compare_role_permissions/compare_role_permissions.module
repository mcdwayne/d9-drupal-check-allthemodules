<?php

/**
 * @file
 * Contains compare_role_permissions.module..
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function compare_role_permissions_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the compare_role_permissions module.
    case 'help.page.compare_role_permissions':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('To compare roles with permissions') . '</p>';
      return $output;

    default:
  }
}
